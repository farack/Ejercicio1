#include <stdio.h>
main(){
int num1, num2, total;
clrscr();
printf("Programa que da la suma, resta, multiplicacion y division");
printf("nn Dame numero 1: ");
scanf("%d",&num1);
printf("n Dame numero 2: ");
scanf("%d",&num2);
total=num1+num2;
printf("nEl resultado de la suma es: %+d",total);
total=num1-num2;
